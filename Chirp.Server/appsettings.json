{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Server=127.0.0.1;Port=5432;Database=chirp;User Id=username;Password=password;"
  },
  "Mail": {
    "Provider": "smtp",
    "Config": {
      "Host": "domain.com",
      "Port": 587,
      "Username": "username@domain.com",
      "Password": "password",
      "EnableSsl": true,
      "Timeout": 30000,
      "FromAddress": "no-reply@domain.com",
      "FromDisplayName": "no-reply"
    }
  },
  "Jwt": {
    "Key": "<jwt key>",
    "Issuer": "https://localhost:5000/",
    "Audience": "https://localhost:5000/"
  }
}
